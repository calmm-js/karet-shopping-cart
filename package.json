{
  "name": "karet-shopping-cart",
  "private": true,
  "description": "Karet Shopping Cart Example",
  "main": "docs/bundle.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "lessc src/components/app.less docs/bundle.css",
    "build:js": "cp node_modules/babel-polyfill/dist/polyfill.min.js docs/bundle.js && NODE_ENV=production browserify src/client/start.js -t babelify -g envify -g uglifyify | uglifyjs -m -c >> docs/bundle.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "npm run watch:js & npm run watch:css & wait",
    "watch:css": "npm run build:css && watch-run -p 'src/components/*.less' 'npm run build:css'",
    "watch:js": "watchify src/client/debug.js -v --loadMaps --debug -t babelify -o docs/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/karet-shopping-cart.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/karet-shopping-cart/issues"
  },
  "homepage": "https://github.com/calmm-js/karet-shopping-cart#readme",
  "dependencies": {
    "atom.storage": "^0.4.3",
    "atom.undo": "^0.4.5",
    "karet": "^0.2.4",
    "karet.util": "^0.8.2",
    "kefir": "^3.7.0",
    "kefir.atom": "^4.0.6",
    "partial.lenses": "^7.2.2",
    "ramda": "^0.23.0",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "envify": "^4.0.0",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "less": "^2.7.2",
    "uglify-js": "^2.7.5",
    "uglifyify": "^3.0.4",
    "watch-run": "^1.2.4",
    "watchify": "^3.8.0"
  }
}
